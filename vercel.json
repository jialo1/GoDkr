{
  "version": 2,
  "builds": [
    {
      "src": "godkr-web/out/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/godkr-web/out/index.html"
    },
    {
      "src": "/discover",
      "dest": "/godkr-web/out/discover.html"
    },
    {
      "src": "/profile",
      "dest": "/godkr-web/out/profile.html"
    },
    {
      "src": "/community",
      "dest": "/godkr-web/out/community.html"
    },
    {
      "src": "/notifications",
      "dest": "/godkr-web/out/notifications.html"
    },
    {
      "src": "/styles.css",
      "dest": "/godkr-web/out/styles.css"
    },
    {
      "src": "/images/(.*)",
      "dest": "/godkr-web/out/images/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/godkr-web/out/$1"
    }
  ],
  "buildCommand": "echo 'No build step required'",
  "installCommand": "echo 'No install step required'"
} 